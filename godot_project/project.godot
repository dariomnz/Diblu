[application]

config/name="Diblu"
run/main_scene="res://scenes/Title Screen.tscn"
run/low_processor_mode=true
boot_splash/image="res://icon.png"
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

global_var="*res://scripts/global_var.gd"
global="*res://scripts/global.gd"
global_signal="*res://scripts/global_signal.gd"
DungeonDB="*res://scripts/Dungeon/DungeonDB.gd"

[display]

mouse_cursor/custom_image="res://assets/GUI/mouse.png"
mouse_cursor/custom_image_hotspot=Vector2( 11, 11 )
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "aseprite_importer" )

